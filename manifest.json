{
  "name": "Gitamite",
  "description": "Extension for gitamites",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://glearn.gitam.edu/student*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png" 
  },
  "permissions": [
    "storage"
  ]
}
